# Maintainer: Baumi Baum <spam@j-baum.at>

pkgname=gnome-schema-override
pkgver=1
pkgrel=0
pkgdesc="Changes some settings on the gnome environment"
arch=('any')
license=('GPL3')
options=(!strip !emptydirs)


package() {

  cd $pkgdir	
  cd ../../
  mkdir -p $pkgdir/usr/share/glib-2.0/schemas/
  install -m644 org.gnome.gschema.override $pkgdir/usr/share/glib-2.0/schemas/	
}

