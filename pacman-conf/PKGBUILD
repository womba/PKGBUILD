pkgname=pacman-conf
pkgver=1
pkgrel=1
pkgdesc="Corn configuration file for pacman"
arch=('any')
license=('GPL')
source=(pacman.conf)


package() {
  mkdir -p $pkgdir/etc
  install -m644 $srcdir/pacman.conf $pkgdir/etc
}

md5sums=('0d1eeea2bfc019dac25df56b633908b8')
sha256sums=('b4d2bc49d1146998f7e1a0a025ff90435e79837a74f5891623230ae32e2c3b82')
